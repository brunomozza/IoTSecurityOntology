    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#hasModeOperation -->

    <owl:AnnotationProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#hasModeOperation"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#affects -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#affects">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Threat"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityProperty"/>
        <rdfs:comment xml:lang="en">A threat affects one or more security properties. For instance, an eavesdropping threat affects the security property Authentication.</rdfs:comment>
        <rdfs:label xml:lang="en">affects</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#generates -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#generates">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Vulnerability"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Risk"/>
        <rdfs:comment xml:lang="en">A vulnerability generates a risk for an organization because it can exposes sensitive information for malicious actions of intruders.</rdfs:comment>
        <rdfs:label xml:lang="en">generates</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#hasFeature -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#hasFeature">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityMechanism"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Feature"/>
        <rdfs:comment xml:lang="en">A security mechanism has specific features that can incapacitate the use with a determined situation. For example, an asymmetric algorithm has high energy consuming and it can to harm the consume of battery of an active RFID.</rdfs:comment>
        <rdfs:label xml:lang="en">hasFeature</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#hasPart -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#hasPart">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityMechanism"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityMechanism"/>
        <rdfs:comment xml:lang="en">A security mechanism can use or be used as part of other mechanism to mitigate an vulnerability or to protect an asset.</rdfs:comment>
        <rdfs:label xml:lang="en">hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#hasSecurityMechanism -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#hasSecurityMechanism">
        <owl:inverseOf rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isSecurityMechanismOf"/>
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Threat"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityMechanism"/>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#hasSeverity -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#hasSeverity">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Vulnerability"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SeverityScale"/>
        <rdfs:comment xml:lang="en">A vulnerability can be classified according to level of severity. For instance, high, medium and low level.</rdfs:comment>
        <rdfs:label xml:lang="en">hasSeverity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#hasType -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#hasType">
        <owl:inverseOf rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isTypeOf"/>
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityMechanism"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#TypeofDefense"/>
        <rdfs:comment xml:lang="en">A security mechanism implements a security tool to protect assets and to mitigate vulnerabilities using distinct type of defense such as: detection, prevention, recovery or corretion.</rdfs:comment>
        <rdfs:label xml:lang="en">hasType</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#hasVulnerability -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#hasVulnerability">
        <owl:inverseOf rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isVulnerabilityOf"/>
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Asset"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Vulnerability"/>
        <rdfs:comment xml:lang="en">An organization has flaws in your assets that can be physical, technical or administrative weakness which it makes vulnerable to attackers.</rdfs:comment>
        <rdfs:label xml:lang="en">hasVulnerability</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isComposedOf -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isComposedOf">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Asset"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Asset"/>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isMitigatedBy -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isMitigatedBy">
        <owl:inverseOf rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#mitigates"/>
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Vulnerability"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityMechanism"/>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isProtectedBy -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isProtectedBy">
        <owl:inverseOf rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#protects"/>
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Asset"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityMechanism"/>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isReplacedBy -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isReplacedBy">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityMechanism"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityMechanism"/>
        <rdfs:comment>A security mechanism was deprecated and a new mechanism replaced it.</rdfs:comment>
        <rdfs:label xml:lang="en">isReplacedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isSecurityMechanismOf -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isSecurityMechanismOf">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityMechanism"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Threat"/>
        <rdfs:comment xml:lang="en">Security mechanisms are developed to mitigates flaws and protect assets. Normally, security mechanisms are defined as countermeasures to block, detect or prevent threats with specific behavior characterized. For instance, normally, firewall mechanism are used to block malicious traffic in network or web application.</rdfs:comment>
        <rdfs:label xml:lang="en">isSecurityMechanismOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isThreatensBy -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isThreatensBy">
        <owl:inverseOf rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#threatens"/>
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Vulnerability"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Threat"/>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isTypeOf -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isTypeOf">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#TypeofDefense"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityMechanism"/>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isVulnerabilityOf -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#isVulnerabilityOf">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Vulnerability"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Asset"/>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#mitigates -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#mitigates">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityMechanism"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Vulnerability"/>
        <rdfs:comment xml:lang="en">Security mechanism are projected to mitigate the asset&apos;s vulnerabilities which can be physical, technical or administrative weakness.</rdfs:comment>
        <rdfs:label xml:lang="en">mitigates</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#occursIn -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#occursIn">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Threat"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#OSIModelLayer"/>
        <rdfs:comment xml:lang="en">A threat occurs in a specific layer of OSI Model. For instance, an eavesdropping attack explores the physical layer.</rdfs:comment>
        <rdfs:label xml:lang="en">occursIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#occursInLayer -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#occursInLayer"/>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#protects -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#protects">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityMechanism"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Asset"/>
        <rdfs:comment xml:lang="en">The security mechanism allows to protect an organization&apos;s asset against malicious actions of the intruders. For instance, the security mechanism called Virtual Private Network (VPN) is used to protect an user when it is using an Wifi &quot;untrusted&quot; network.</rdfs:comment>
        <rdfs:label xml:lang="en">protects</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#protectsInLayer -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#protectsInLayer"/>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#requires -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#requires">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Asset"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityProperty"/>
        <rdfs:comment xml:lang="en">Organizations&apos; assets have requirements to work normally, involving the main security  properties. For instance, a web server need maintain the service available to users in all the time. For that, this security property is required by web server. In case of one threat attacks the web server, this security property will be affected.</rdfs:comment>
        <rdfs:label xml:lang="en">requires</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#satisfies -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#satisfies">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityMechanism"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SecurityProperty"/>
        <rdfs:comment xml:lang="en">Security mechanisms are designed to protect assets and to mitigate vulnerabilities, but in some situations they can not ensure security properties. In this context, the composition of several security mechanisms are used to cover them. In several situations, different security mechanisms are used to protect an organization&apos; asset. For instances, the TinySec is first fully-implemented link-layer security protocol to wireless sensor network (WSN) which have limited power resources, battery life and communication capabilities to guarantee authenticity, integrity and confidentiality. This protocol protects against resource consumption attacks, physical tamper resistance and node capture attacks. By another side, the TinySec protocol does not protect against replay attacks because it does not have the resources for this. So, the solution is add another higher level security protocol to solve this problem.</rdfs:comment>
        <rdfs:label xml:lang="en">satisfies</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#threatens -->

    <owl:ObjectProperty rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#threatens">
        <rdfs:domain rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Threat"/>
        <rdfs:range rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Vulnerability"/>
        <rdfs:comment xml:lang="en">Threats are created to explores the physical, technical and administrative weakness. Many of them are used in one vulnerability already not explored and, it normally explores a flaw recently discovered (zero-day vulnerability) by a hacker. This threat will be efficiently until a security mechanism is designed to block it or the flaw is corrected.</rdfs:comment>
        <rdfs:label xml:lang="en">threatens</rdfs:label>
    </owl:ObjectProperty>
    


