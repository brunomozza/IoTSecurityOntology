        <rdfs:subClassOf rdf:resource="&iotsec;SecurityFeatures"/>
        <rdfs:comment xml:lang="en">When an actor claims to have a given identity, the software does not prove or insufficiently proves that the claim is correct [CWE-287].</rdfs:comment>
        <rdfs:seeAlso xml:lang="en">http://cwe.mitre.org/data/definitions/287.html</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec/iotsec.owl#ImproperInputValidation -->

