    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#ConnectivityDevicesMode4WithOthers -->

    <owl:NamedIndividual rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#ConnectivityDevicesMode4WithOthers">
        <rdf:type rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#BluetoothVulnerability"/>
        <dc:description xml:lang="en">The worst-case scenario would be a device falling back to Security Mode 1, which provides no security. NIST strongly recommends that a Security Mode 4 device fall back to Security Mode 3 in this scenario. Versions Bluetooth v2.1 and v3.0</dc:description>
        <rdfs:label xml:lang="en">Security Mode 4 devices (i.e., v2.1 or later) are allowed to fall back to any other security mode when connecting with devices that do not support Security Mode 4 (i.e., v2.0 and earlier).</rdfs:label>
    </owl:NamedIndividual>
    


