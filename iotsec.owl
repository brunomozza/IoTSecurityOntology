    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#DiscoveredDevicesPronetoAttack -->

    <owl:NamedIndividual rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#DiscoveredDevicesPronetoAttack">
        <rdf:type rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#BluetoothVulnerability"/>
        <dc:description xml:lang="en">Any device that must go into discoverable or connectable mode to pair or connect should only do so for a minimal amount of time. A device should not be in discoverable or connectable mode all the time [Padgette 2012]. All versions</dc:description>
