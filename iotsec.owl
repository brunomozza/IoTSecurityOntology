        <rdfs:comment xml:lang="en"></rdfs:comment>
        <rdfs:label xml:lang="en">Security Hash Algorithm (RIPE-MD)</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#RSA -->

    <owl:NamedIndividual rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#RSA">
        <rdf:type rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#AsymmetricAlgorithm"/>
        <rdf:type rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#DigitalSignature"/>
        <rdf:type rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#KeyManagement"/>
        <rdf:type rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#ManetSecurityMechanism"/>
        <satisfies rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Authentication"/>
        <satisfies rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#Confidentiality"/>
        <rdfs:comment xml:lang="en"></rdfs:comment>
        <rdfs:label xml:lang="en">Rivest Shamir Adleman (RSA)</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www-usr.inf.ufsm.br/~brunomozza/iotsec#RaspberryPi -->

    <owl:NamedIndividual rdf:about="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#RaspberryPi">
        <rdf:type rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#SensorTechnology"/>
        <hasVulnerability rdf:resource="http://www-usr.inf.ufsm.br/~brunomozza/iotsec#UnprotectedCommunicationChannels"/>
